r","setTimeout","window","location","reload"],"sources":["C:/Users/Henry/Desktop/GreenlogistApps-main/GreenlogistApps-main/src/App.vue"],"sourcesContent":["  <script setup>\n  import { useRouter } from 'vue-router';\n  const router = useRouter();\n  const handleLogout = () => {\n  localStorage.removeItem('token'); \n  sessionStorage.clear(); \n  router.push('/login'); \n  setTimeout(() => window.location.reload(), 100);\n  };\n</script>\n\n\n<template>\n  <div id=\"app\">\n    <div class=\"app-container\">\n      <aside class=\"app-sidebar\" v-if=\"showSidebar\">\n        <div class=\"sidebar-header\">\n          <h2>GreenLogist</h2>\n          <p>Panel de Control</p>\n        </div>\n\n        <nav class=\"sidebar-nav\">\n          <router-link to=\"/dashboard\">\n            <i class=\"fas fa-home\"></i> Dashboard\n          </router-link>\n          <router-link to=\"/registrar-producto\">\n            <i class=\"fas fa-box\"></i> Registrar Producto\n          </router-link>\n          <router-link to=\"/productores\">\n            <i class=\"fas fa-users\"></i> Solicitud de Transporte\n          </router-link>\n          <router-link to=\"/estadisticas\">\n            <i class=\"fas fa-chart-bar\"></i> Estadísticas\n          </router-link>\n          <router-link to=\"/perfil\">\n            <i class=\"fas fa-user\"></i> Mi Perfil\n          </router-link>\n        </nav>\n          <router-link to=\"/LoginView\">\n            <i class=\"fas fa-user\"></i> Cerrar Sesión\n          </router-link>\n        <button class=\"logout-btn\" @click=\"handleLogout\">\n          <i class=\"fas fa-sign-out-alt\"></i> Cerrar Sesión\n        </button>\n      </aside>\n\n   \n      <main class=\"app-content\">\n        <router-view />\n      </main>\n    </div>\n\n\n    <NotificationContainer ref=\"notificationContainer\" />\n  </div>\n</template>\n\n<script>\nimport NotificationContainer from '@/components/NotificationContainer.vue'\nimport { useRouter } from 'vue-router'\n\nexport default {\n  name: 'App',\n  components: {\n    NotificationContainer\n  },\n  setup() {\n    const router = useRouter()\n    \n    const handleLogout = () => {\n      localStorage.removeItem('currentUser')\n      router.push('/login')\n      this.showNotification('success', 'Sesión cerrada correctamente')\n    }\n\n    return {\n      handleLogout\n    }\n  },\n  computed: {\n    showSidebar() {\n      return !['/login', '/register'].includes(this.$route.path)\n    }\n  },\n  methods: {\n    showNotification(type, message, duration = 5000) {\n      this.$refs.notificationContainer.addNotification({\n        type,\n        message,\n        duration\n      })\n    }\n  },\n  provide() {\n    return {\n      showNotification: this.showNotification\n    }\n  }\n}\n</script>\n\n<style>\n\n:root {\n  --primary-color: #2E7D32;\n  --primary-dark: #1B5E20;\n  --primary-light: #81C784;\n  --sidebar-width: 250px;\n  --header-height: 60px;\n}\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n  background-color: #f5f5f5;\n}\n\n#app {\n  min-height: 100vh;\n}\n\n\n.app-container {\n  display: flex;\n  min-height: 100vh;\n}\n\n\n.app-sidebar {\n  width: var(--sidebar-width);\n  background-color: white;\n  box-shadow: 2px 0 10px rgba(0, 0, 0, 0.1);\n  display: flex;\n  flex-direction: column;\n  padding: 1rem;\n}\n\n.sidebar-header {\n  padding: 1rem;\n  margin-bottom: 2rem;\n  border-bottom: 1px solid #eee;\n}\n\n.sidebar-header h2 {\n  color: var(--primary-color);\n  margin-bottom: 0.5rem;\n}\n\n.sidebar-nav {\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n\n.sidebar-nav a {\n  padding: 0.75rem 1rem;\n  border-radius: 4px;\n  color: #333;\n  text-decoration: none;\n  display: flex;\n  align-items: center;\n  gap: 0.75rem;\n  transition: all 0.2s;\n}\n\n.sidebar-nav a:hover {\n  background-color: #f0f0f0;\n}\n\n.sidebar-nav a.router-link-exact-active {\n  background-color: var(--primary-light);\n  color: var(--primary-dark);\n  font-weight: 500;\n}\n\n.logout-btn {\n  margin-top: auto;\n  padding: 0.75rem;\n  background: none;\n  border: none;\n  color: #D32F2F;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  gap: 0.75rem;\n  border-radius: 4px;\n}\n\n.logout-btn:hover {\n  background-color: #ffebee;\n}\n\n/* Contenido Principal */\n.app-content {\n  flex: 1;\n  padding: 2rem;\n  background-color: #f9f9f9;\n}\n\n/* Responsive */\n@media (max-width: 768px) {\n  .app-sidebar {\n    width: 70px;\n    padding: 0.5rem;\n  }\n  \n  .sidebar-nav a span, \n  .sidebar-header p,\n  .logout-btn span {\n    display: none;\n  }\n  \n  .sidebar-nav a,\n  .logout-btn {\n    justify-content: center;\n  }\n}\n</style>"],"mappings":";AA0DA,OAAOA,qBAAqB,MAAM,wCAAuC;AACzE,SAASC,SAAS,QAAQ,YAAW;AAErC,MAAAC,WAAA,GAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAM,GAAGL,SAAS,CAAC;IAEzB,MAAMM,YAAY,GAAGA,CAAA,KAAM;MACzBC,YAAY,CAACC,UAAU,CAAC,aAAa;MACrCH,MAAM,CAACI,IAAI,CAAC,QAAQ;MACpB,IAAI,CAACC,gBAAgB,CAAC,SAAS,EAAE,8BAA8B;IACjE;IAEA,OAAO;MACLJ;IACF;EACF,CAAC;EACDK,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,CAAC,CAAC,QAAQ,EAAE,WAAW,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,IAAI;IAC3D;EACF,CAAC;EACDC,OAAO,EAAE;IACPN,gBAAgBA,CAACO,IAAI,EAAEC,OAAO,EAAEC,QAAQ,GAAG,IAAI,EAAE;MAC/C,IAAI,CAACC,KAAK,CAACC,qBAAqB,CAACC,eAAe,CAAC;QAC/CL,IAAI;QACJC,OAAO;QACPC;MACF,CAAC;IACH;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,OAAO;MACLb,gBAAgB,EAAE,IAAI,CAACA;IACzB;EACF;AACF;;;;;;IAhGE,MAAML,MAAM,GAAGL,SAAS,CAAC,CAAC;IAC1B,MAAMM,YAAY,GAAGA,CAAA,KAAM;MAC3BC,YAAY,CAACC,UAAU,CAAC,OAAO,CAAC;MAChCgB,cAAc,CAACC,KAAK,CAAC,CAAC;MACtBpB,MAAM,CAACI,IAAI,CAAC,QAAQ,CAAC;MACrBiB,UAAU,CAAC,MAAMC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC,EAAE,GAAG,CAAC;IAC/C,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}